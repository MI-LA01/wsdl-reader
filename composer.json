{
    "name": "goetas/wsdl-reader",
    "description": "Pure PHP WSDL parser",
    "keywords": ["xsd", "xml", "soap", "webservice", "wsdl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@gmail.com"
        }
    ],
    "require": {
        "php": "~5.4",
        "goetas/xsd-reader": "~2.0@dev",
        "symfony/event-dispatcher" : "~2.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0",
        "sami/sami" : "~2.0"
    },
       
    "autoload": {
        "psr-4": { "Goetas\\XML\\WSDLReader\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Goetas\\XML\\WSDLReader\\Tests\\": "tests/" }
    },    
    "extra":{
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
