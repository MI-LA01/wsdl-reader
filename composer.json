{
    "name": "php-webservices/wsdl-reader",
    "description": "Pure PHP WSDL parser",
    "keywords": ["xsd", "xml", "soap", "webservice", "wsdl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@gmail.com"
        }
    ],
    "require": {
        "php": "~5.4",
        "php-webservices/xsd-reader": "~0.1",
        "symfony/event-dispatcher" : "~2.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0",
        "sami/sami" : "~2.0",
        "jmikola/wildcard-event-dispatcher": "~1.0"
    },
    
    "autoload": {
        "psr-4": { "PhpWebservices\\XML\\WSDLReader\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "PhpWebservices\\XML\\WSDLReader\\Tests\\": "tests/" }
    },    
    "extra":{
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
